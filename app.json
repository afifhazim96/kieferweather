{
  "expo": {
    "name": "Kieferweather",
    "slug": "kieferweather",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#232323"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "media.kiefer.weather",
      "versionCode": 3,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "backgroundImage": "./assets/android_bg.png",
        "foregroundImage": "./assets/android_fg.png"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "description": "Always the best weather for your location!"
  }
}
